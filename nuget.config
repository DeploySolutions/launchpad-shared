<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="DeploySolutionsFactory (GitHub)" value="https://nuget.pkg.github.com/DeploySolutionsFactory/index.json" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="Microsoft.*" />
      <package pattern="System.*" />
      <package pattern="Newtonsoft.Json" />
      <package pattern="Abp.*" />
      <package pattern="Deploy.*" />
    </packageSource>
    <packageSource key="DeploySolutionsFactory (GitHub)">
      <package pattern="Deploy.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>